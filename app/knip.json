    "assets/js/models/companies/hasFeature.tsx",
    "assets/js/components/Callouts/index.tsx",
    "assets/js/routes/redirectUtils.tsx"
  ],
  "ignoreDependencies": ["@types/jest", "turboui"],

  "rules": {
    "nsTypes": "off"
  }
}

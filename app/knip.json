{
  "$schema": "https://unpkg.com/knip@2/schema.json",
  "entry": ["assets/js/app.tsx", "assets/build.js", "assets/codegen.ts"],
  "project": ["assets/js/**/*.ts", "assets/js/**/*.tsx", "ee/assets/js/**/*.ts", "ee/assets/js/**/*.tsx"],
  "ignore": [
    "vite.config.mjs",

    "assets/js/api/index.tsx",
    "ee/assets/js/admin_api/index.tsx",

{
  "app/test/features/*_test.exs": {
    "type": "test",
    "alternate": "app/test/support/features/{}_steps.ex",
    "template": ["# app/test/{}_test.exs", "defmodule XXX do", "end"]
  },
  "app/assets/js/pages/*/index.tsx": {
    "type": "page",
    "alternate": "turboui/src/{}/index.tsx"
  },
  "turboui/src/*/index.tsx": {
    "type": "page",
    "alternate": "app/assets/js/pages/{}/index.tsx"
  },
  "app/test/support/features/*_steps.ex": {
    "type": "test",
    "alternate": "app/test/features/{}_test.exs",
    "template": ["# app/test/{}_test.exs", "defmodule XXX do", "end"]
  },
  "app/lib/*.ex": {
    "type": "src",
    "alternate": "app/test/{}_test.exs",
    "template": ["# app/lib/{}.ex", "defmodule XXX do", "end"]
  },
  "app/test/*_test.exs": {
    "type": "test",
    "alternate": "app/lib/{}.ex",
    "template": ["# app/test/{}_test.exs", "defmodule XXX do", "end"]
  },
  "app/ee/lib/*.ex": {
    "type": "src",
    "alternate": "app/ee/test/lib/{}_test.exs",
    "template": ["# app/ee/lib/{}.ex", "defmodule OperatelyEE.XXX do", "end"]
  },
  "app/ee/test/lib/*_test.exs": {
    "type": "test",
    "alternate": "app/ee/lib/{}.ex",
    "template": ["# app/ee/test/lib/{}_test.exs", "defmodule OperatelyEE.XXXTest do", "  use Operately.DataCase", "", "  # tests go here", "end"]
  },
  "app/ee/lib/admin_api/*.ex": {
    "type": "src",
    "alternate": "app/ee/test/operately_ee/admin_api/{}_test.exs",
    "template": ["# app/ee/lib/admin_api/{}.ex", "defmodule OperatelyEE.XXX do", "end"]
  },
  "app/ee/test/operately_ee/admin_api/*_test.exs": {
    "type": "test",
    "alternate": "app/ee/lib/admin_api/{}.ex",
    "template": ["# app/ee/test/operately_ee/admin_api/{}_test.exs", "defmodule OperatelyEE.XXXTest do", "  use Operately.DataCase", "", "  # tests go here", "end"]
  }
}
